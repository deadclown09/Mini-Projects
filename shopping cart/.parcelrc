    "*.js": ["@parcel/transformer-js"]
  },
  "packagers": {
    "*.html": "@parcel/packager-html",
    "*.js": "@parcel/packager-js"
  },
  "optimizers": {
    "*.html": ["@parcel/optimizer-htmlnano"],
    "*.js": ["@parcel/optimizer-terser"]
  },
  "publicUrl": "./"
}